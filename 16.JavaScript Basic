9.Operation
    var x = 99;
    x == "99" // true
    x === "99" // false (type also need to be same)
    var x = null;
    x == underfined     // true
    x === underfined    // false
    
    true == "1"         // true
    0 == false         // true
    NaN == NaN         // true
    
    %%  and
    ||  or
    !   not
    
    !" "    // false
    !""     // true
    !0      // true
    // you can use !!+what you wanna check to check its boolean
    // "false" sting true ot false is truthy!
    // sum up: false: null undefined NaN "" 0 false
    
    
